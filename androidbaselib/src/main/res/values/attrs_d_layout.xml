<resources>
    <attr name="backgroudColor" format="color"/>
    <declare-styleable name="DLayout">
        <attr name="textSize"/>
        <attr name="backgroudColor"/>
        <attr name="text"/>
        <attr name="rowSpace"/>
        <attr name="columnSpace"/>
        <attr name="column" format="integer"/>
    </declare-styleable>

    <declare-styleable name="DViewGridView">
        <attr name="text"/>
        <attr name="textSize"/>
    </declare-styleable>

    <declare-styleable name="DView">
        <attr name="textSize"/>
        <attr name="text"/>
        <attr name="textColor" format="color"/>
        <attr name="lineColor" format="color"/>
    </declare-styleable>
</resources>
